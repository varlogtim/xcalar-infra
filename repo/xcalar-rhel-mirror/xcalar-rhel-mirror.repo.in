[xcalar-rhel-mirror]
name=Xcalar RHEL $releasever Mirror Repository
baseurl=@URL@/$releasever/rhui
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-Xcalar-RHEL-Mirror

[xcalar-epel-mirror]
name=Xcalar EPEL $releasever Mirror Repository
baseurl=@URL@/$releasever/epel
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-Xcalar-EPEL6-Mirror

[xcalar-ius-mirror]
name=Xcalar IUS $releasever Mirror Repository
baseurl=@URL@/$releasever/ius
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-Xcalar-IUS-Mirror
