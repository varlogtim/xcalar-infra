[gerrit]
    basePath = git
    canonicalWebUrl = https://gerrit.int.xcalar.com
[database]
    type = H2
    database = db/ReviewDB
[auth]
    type = OpenID
[sendemail]
    smtpServer = localhost
[container]
    user = gerrit
[sshd]
    listenAddress = *:29418
[httpd]
    listenUrl = http://*:8080/
[cache]
    directory = cache
